<robot name="abb_arm">
  <!-- Include materials -->
  <material name="Black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="Red">
    <color rgba="0.8 0.0 0.0 1.0"/>
  </material>
  <material name="White">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>
  <material name="Yellow">
    <color rgba="0.5 0.5 0.0 1.0"/>
  </material>

  <!-- DUMMY LINK -->
  <link name="base_dummy_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0 0 0"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".5"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!-- BASE LINK -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.015"/>
      <geometry>
        <box size="0.095 0.077 0.02"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.015"/>
      <geometry>
        <box size="0.095 0.077 0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".5"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!-- SHOULDER LINK -->
  <link name="shoulder_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <geometry>
        <box size="0.12 0.06 0.05"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.025"/>
      <geometry>
        <box size="0.12 0.06 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!-- BIGARM LINK -->
  <link name="bigarm_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.065"/>
      <geometry>
        <box size="0.04 0.04 0.13"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.065"/>
      <geometry>
        <box size="0.04 0.04 0.13"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!--ELBOW LINK -->
  <link name="elbow_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.01"/>
      <geometry>
        <cylinder length="0.02" radius="0.02"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.01"/>
      <geometry>
        <cylinder length="0.02" radius="0.02"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!--FOREARM LINK -->
  <link name="forearm_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.08"/>
      <geometry>
        <box size="0.025 0.025 0.16"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.08"/>
      <geometry>
        <box size="0.025 0.025 0.16"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!--HAND LINK -->
  <link name="hand_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.015"/>
      <geometry>
        <box size="0.02 0.01 0.03"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.015"/>
      <geometry>
        <box size="0.02 0.01 0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!--endeffector -->
  <link name="endeffector">
    <visual>
      <origin rpy="0 0 0" xyz="-0.04 0 0.0025"/>
      <geometry>
        <box size="0.08 0.005 0.005"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.04 0 0.0025"/>
      <geometry>
        <box size="0.08 0.005 0.005"/>
      </geometry>
    </collision>
    <inertial>
      <mass value=".1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="0.5" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!-- ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->
  <joint name="base_dummy_joint" type="fixed">
    <parent link="base_dummy_link"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis xyz="0 0 0"/>
  </joint>
  <joint name="shoulder_joint" type="revolute">
    <parent link="base_link"/>
    <child link="shoulder_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.03"/>
    <axis xyz="0 0 1"/>
    <limit effort="300" lower="-2.094" upper="2.094" velocity="1"/>
    <dynamics damping="50" friction="1"/>
  </joint>
  <joint name="bigarm_joint" type="revolute">
    <parent link="shoulder_link"/>
    <child link="bigarm_link"/>
    <origin rpy="0 0 0" xyz="0.04 0 0.05"/>
    <axis xyz="0 1 0"/>
    <limit effort="300" lower="-1.48353" upper="1.48353" velocity="1"/>
    <dynamics damping="50" friction="1"/>
  </joint>
  <joint name="elbow_joint" type="revolute">
    <parent link="bigarm_link"/>
    <child link="elbow_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.13"/>
    <axis xyz="0 1 0"/>
    <limit effort="300" lower="0.785" upper="2.356" velocity="1"/>
    <dynamics damping="50" friction="1"/>
  </joint>
  <joint name="forearm_joint" type="revolute">
    <parent link="elbow_link"/>
    <child link="forearm_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.02"/>
    <axis xyz="0 0 1"/>
    <limit effort="300" lower="-2.094" upper="2.094" velocity="1"/>
    <dynamics damping="50" friction="1"/>
  </joint>
  <joint name="hand_joint" type="revolute">
    <parent link="forearm_link"/>
    <child link="hand_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.16"/>
    <axis xyz="0 1 0"/>
    <limit effort="10" lower="-2.094" upper="2.094" velocity="1"/>
    <dynamics damping="50" friction="1"/>
  </joint>
  <joint name="endeff_joint" type="revolute">
    <parent link="hand_link"/>
    <child link="endeffector"/>
    <!-- <child link="right_finger_link"/> -->
    <origin rpy="0 0 0" xyz="0 0 0.03"/>
    <axis xyz="0 0 1"/>
    <limit effort="10" lower="-2.094" upper="2.094" velocity="1"/>
    <dynamics damping="50" friction="1"/>
  </joint>
</robot>

